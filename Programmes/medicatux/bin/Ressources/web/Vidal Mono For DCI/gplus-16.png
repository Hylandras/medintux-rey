    <script type="text/javascript">
        
        var d = $("rating_stars");
        if (d) {
            var radios = d.select("input[type=radio]");
            if (radios.length)
            {
                d.innerHTML = "";
                var n;
                var v = 0;
                radios.each(function(r){
                    var s = document.createElement("div");
                    s.className = "rating_star on";
                    s.setAttribute("id", r.id);
                    s.innerHTML = r.value;
                    d.appendChild(s);
                    n = r.name;
                    if (r.checked) v = r.value;
                });
                d.innerHTML += '<input type="hidden" id="rating_input" name="'+n+'" value="'+v+'">';
            }
            var stars = d.select(".rating_star");
            stars.each(function(r) {
                r.observe("click", function(e){
                    $("rating_input").value = this.innerHTML;
                });
                r.observe("mouseover", function(e){
                    for(var i = 0 ; i < stars.length ; i++)
                    {
                        var v1 = stars[i].innerHTML;
                        var v2 = this.innerHTML;
                        if (v1 <= v2) stars[i].addClassName("on");
                        else stars[i].removeClassName("on");
                    }
                });
                r.observe("mouseout", function(e){
                    for(var i = 0 ; i < stars.length ; i++)
                    {
                        var v1 = stars[i].innerHTML;
                        var v2 = $("rating_input").value;
                        if (v1 <= v2) stars[i].addClassName("on");
                        else stars[i].removeClassName("on");
                    }
                });
            });

        }
        
    </script>
